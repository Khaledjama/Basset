        </div>


        <div class="py-2 bg-light">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-3 text-right">
                        @if (User.Identity.IsAuthenticated)
                        {
                            <a asp-action="LogOut" asp-controller="Account" style="color: #51be78;text-align: center;text-decoration: none;" class="small mr-3"><span class="icon-unlock-alt"></span> تسجيل الخروج</a>
                        }
                        else{

                            <a asp-action="Login" asp-controller="Account" style="color: #51be78;text-align: center;text-decoration: none;" class="small mr-3"><span class="icon-unlock-alt"></span> تسجيل الدخول</a>


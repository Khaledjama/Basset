                    </div>

                </div>
            </div>
        </div>
        <header id="head" class="site-navbar py-4 js-sticky-header site-navbar-target" role="banner">

            <div class="container">
                <div class="d-flex align-items-center">
                    <div class="site-logo">
                        <a asp-action="Index" asp-controller="Home" class="d-block">
                            <img src="~/SecondTemplate/Academic-Master/images/Logo.jpeg" style="height:55px" alt="Image" class="img-fluid">
                        </a>
                    </div>
                    <div class="mr-auto">
                        <nav class="site-navigation position-relative text-right" role="navigation">
                            <ul class="site-menu main-menu js-clone-nav mr-auto d-none d-lg-block" style="font-size: 20px;font-weight: 200;">
                                <li class="active">
                                    <a asp-action="Index" asp-controller="Home" class="nav-link text-left js-closhome" id="ho">الصفحة الرئيسية</a>
                                </li>
                                @if (User.IsInRole("الادمن"))
                                {
                                    <li class="has-children">

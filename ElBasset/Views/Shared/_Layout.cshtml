                                            <li><a asp-action="AllExam" asp-controller="Exams" class="js-closhome">الامتحانات </a></li>
                                            <li><a asp-action="AllVideo" asp-controller="Video" class="js-closhome">الفيديوهات </a></li>
                                            <li><a asp-action="AllLecture" asp-controller="Lecture" class="js-closhome">الدروس </a></li>

                                        </ul>
                                    </li>
                                }

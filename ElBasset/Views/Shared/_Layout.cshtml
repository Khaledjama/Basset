                            <a href="https://api.whatsapp.com/send?phone=201157462787"><span class="icon-phone mr-2"></span></a>
                            <a  asp-action="Privacy" asp-controller="Home" ><span class="icon-person"></span></a>
                            <a href="#" class="d-inline-block d-lg-none site-menu-toggle js-menu-toggle text-black">
                                <span class="icon-menu h3"></span>
                            </a>
                        </div>
                    </div>

                </div>
            </div>

        </header>
        <div>
            @RenderBody()
